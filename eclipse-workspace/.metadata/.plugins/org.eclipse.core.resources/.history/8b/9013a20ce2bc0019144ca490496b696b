package com.rbs.spcore;

public class Address {

	public String city;
	public String state;
	public String country;
	
	public Address(String city, String state, String country) {
		super();
		this.city =city;
		this.state=state;
		this.country=country;
	}
	
	public String toString() {
		return city+" "+state+" "+country;
	}
}
